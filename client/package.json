{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "SET NODE_OPTIONS=--openssl-legacy-provider && vite",
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && tsc && vite build",
    "dev:server": "node ./server/server.cjs",
    "preview": "vite preview",
    "test:unit": "vitest --mode development",
    "lint:ts": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "storybook": "SET NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006 -c ./config/storybook",
    "build-storybook": "build-storybook -c ./config/storybook",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "i18next": "^22.4.11",
    "i18next-http-backend": "^2.1.1",
    "jwt-decode": "^3.1.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "uuid": "^9.0.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-loader": "^8.3.0",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-roweens-plugin": "^0.0.6",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "jsdom": "^21.1.1",
    "json-server": "^0.17.2",
    "loki": "^0.31.0",
    "sass": "^1.59.2",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.0-beta.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
